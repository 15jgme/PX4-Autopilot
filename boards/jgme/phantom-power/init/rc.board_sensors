#!/bin/sh
#
# Bitcraze Crazyflie specific board sensors init
#------------------------------------------------------------------------------

board_adc start

# Internal I2C bus
bmp280 -X -a 0x76 -f 400 start
bmi055_i2c -X -A -R 0 -a 0x18 start
bmi055_i2c -X -G -R 0 -a 0x68 start
# bmm150 -X -R 0 start
bmm150 -X -b 1 -R 2 start
